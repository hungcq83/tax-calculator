{
  "family": "tax-calculator",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::404773554099:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "tax-calculator",
      "image": "404773554099.dkr.ecr.us-east-1.amazonaws.com/tax-calculator:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": null,
      "command": null
    }
